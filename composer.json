{
    "name": "thiefterminator/flarum-japanese",
    "type": "flarum-extension",
    "description": "Japanese language pack.",
    "keywords": ["locale", "japanese"],
    "license": "MIT",
    "require": {
        "flarum/core": "^2.0"
    },
    "authors": [
        {
            "name": "ThiefTerminator",
            "homepage": "https://github.com/ThiefTerminator"
        }
    ],
    "extra": {
        "flarum-extension": {
            "title": "Flarum Japanese",
            "category": "language",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            },
            "locale": "ja"
